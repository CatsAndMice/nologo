{
    "manifest_version": 3,
    "name": "去水印下载鸭",
    "version": "1.0.1",
    "description": "帮助用户下载社交媒体平台的去水印内容",
    "permissions": [
        "activeTab",
        "tabs",
        "cookies",
        "webRequest"
    ],
    "action": {
        "default_icon": {
            "16": "logo.png",
            "48": "logo.png",
            "128": "logo.png"
        }
    },
    "icons": {
        "16": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
    },
    "host_permissions": [
        "*://*/*"
    ],
    "background": {
        "service_worker": "background.js"
    }
}